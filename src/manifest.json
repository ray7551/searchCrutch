{
  "name": "搜索拐杖",
  "version": "3.95",
  "description": "在 Google，百度，必应等等搜索引擎间进行快捷切换，搜索关键词无需重输！",
  "homepage_url": "http://xuhehuan.com/2272.html",
  "background": {
    "scripts": [
      "js/common.js",
      "js/background.js"
    ]
  },
  "content_security_policy": "script-src 'self'; form-action 'self'; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "19": "img/icon-19.png",
      "38": "img/icon-38.png"
    },
    "default_title": "搜索拐杖",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "unlimitedStorage",
    "<all_urls>"
  ],
  "options_ui": {
    "page": "setting.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "64": "img/icon-64.png",
    "128": "img/icon-128.png"
  },
  "manifest_version": 2
}